digraph {
	p1
		p1 -> p2 [label="req+"]
	p3
		p2 -> p3 [label="sendline+"]
	p4
		p3 -> p4 [label="[done-]ackline+"]
	p5
		p4 -> p5 [label="sendline-"]
	p6
		p5 -> p6 [label="ackline-"]
	p3
		p6 -> p3 [label="sendline+"]
		p3 -> p7 [label="[done+]ackline+"]
	p8
		p7 -> p8 [label="sendline-ack+"]
	p9
		p8 -> p9 [label="req-ackline-"]
	p1
		p9 -> p1 [label="ack-"]
}