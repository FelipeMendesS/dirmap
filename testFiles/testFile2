.name scsi_targ_send
.inputs ackin 0 dack 0 done 0 dtc 0 startdmasend 0
.outputs dreq 0 endmaint 1 ready 0 regout 0
.choice empty 0
p1/p2|startdmasend+
p2/p3|endmaint-
p3/p4|startdmasend-
p4/p5|dreq+
p5/p6|dack+
p6,p24/p7|ready+
p7/p8|#done+,dtc+
p8/p9|ready-,dreq-
p9/p10|dack-,dtc-
p10/p11|regout+
p11/p12|ackin+
p12/p13|regout-
p13/p14|#empty-,ackin-
p14/p1|endmaint+
p15/p11|ackin+
p7/p25|#done-, dtc+
p25/p16|ready-
p16/p17|dack-,dtc-
p17/p18|regout+
p18/p19|dack*,ackin+
p19/p20|dack*, regout-
p20/p21|#empty-,dack*,ackin-
p21/p22|dack*,regout+
p22/p23|dack*,ackin+
p23/p20|dack*, regout-
p20/p24|#empty+,dack+,ackin-
.start p1
.end
